{
  "name": "metro-performance-dashboard",
  "version": "2.0.0",
  "description": "Real-time performance monitoring dashboard for Metro React Native builds",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-server.js",
    "build": "echo 'Dashboard is client-side only, no build needed'",
    "install-dependencies": "npm install",
    "setup": "node setup.js",
    "benchmark": "curl -X POST http://localhost:3000/api/benchmark -H 'Content-Type: application/json' -d '{\"platform\":\"android\",\"environment\":\"production\"}'",
    "health": "curl http://localhost:3000/health"
  },
  "keywords": [
    "metro",
    "react-native",
    "performance",
    "monitoring",
    "dashboard",
    "benchmarking",
    "build-tools",
    "developer-tools"
  ],
  "author": "EchoTrail Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/echotrail/metro-optimization.git"
  },
  "bugs": {
    "url": "https://github.com/echotrail/metro-optimization/issues"
  },
  "homepage": "https://github.com/echotrail/metro-optimization#readme"
}